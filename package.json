    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "deepmerge": "^4.3.1",
    "discord.js": "^14.15.3",
    "dot-object": "^2.1.4",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.31.2",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "figlet": "^1.7.0",
    "glob": "^10.4.1",
    "jpeg-js": "^0.4.4",
    "json5": "^2.2.3",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "nsfwjs": "^3.0.0",
